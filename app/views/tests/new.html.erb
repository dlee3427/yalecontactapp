<body>
  <div class="container">
    <div class="row">
      <div class="col-lg-10 col-xl-9 mx-auto">
        <div class="card card-signin flex-row my-5">
          <div class="card-img-left d-none d-md-flex">
             <!-- Background image for card set in CSS! -->
          </div>
            <div class="card-body">
                <h5 class="card-title text-center">Test Results</h5>
                <form>
                    <div class="form-group row">
                        <label for="name" class="col-sm-2 col-form-label"></label>
                        <div class="col-sm-10">
                            <input type="name" class="form-control" id="user" placeholder="Name"><br>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="date" class="col-2 col-form-label"></label>
                        <div class="col-10">
                            <input class="form-control" type="date" value="2020-06-30" id="datetime"><br>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="facility" class="col-2 col-form-label"></label>
                        <div class="col-10">
                            <input class="form-control" type="facility" id="facility" placeholder="Facility" ><br>
                        </div>
                    </div>
                    <div class="form-group row">
                      <div class="col-md-12 text-center">
                      <button type="submit" class="btn btn-primary">Submit</button>
                      </div>
                    </div>
                </form>
            </div>
          </div>
        </div>
      </div>
</body>